<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER"
      maxHeight="-Infinity"
      maxWidth="-Infinity"
      minHeight="-Infinity"
      minWidth="-Infinity"
      prefWidth="320.0"
      spacing="8.0"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.speedment.tool.core.internal.controller.AboutController">
    <children>
        <ImageView fx:id="titleImage" fitHeight="32.0" preserveRatio="true" smooth="true" />
        <Label fx:id="version" alignment="CENTER" maxWidth="1.7976931348623157E308" text="Version 2.2" />
        <Label fx:id="license" alignment="CENTER" maxWidth="1.7976931348623157E308" text="{title} includes software licensed under the following terms:" wrapText="true" />
        <Label fx:id="external" alignment="CENTER" maxWidth="1.7976931348623157E308" text="" wrapText="true" />
        <HBox alignment="CENTER_RIGHT">
            <children>
                <Button fx:id="close" defaultButton="true" mnemonicParsing="false" text="Ok, got it!">
                    <padding>
                        <Insets bottom="4.0" left="8.0" right="8.0" top="4.0" />
                    </padding>
                </Button>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
    </padding>
</VBox>
